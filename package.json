{
  "name": "rck",
  "version": "0.1.1",
  "description": "React UI Component Kit",
  "main": "lib/index.js",
  "module": "es/index.js",
  "scripts": {
    "build-umd": "webpack",
    "build-es": "gulp copy-to-es",
    "build-cjs": "gulp build-cjs",
    "clean": "rimraf publish",
    "build": "npm run clean && mkdir publish && npm run build-umd && npm run build-es && npm run build-cjs && gulp generate-extra-file",
    "publish": "cd publish && npm publish",
    "dev": "gulp compile-scss && npm run storybook",
    "precommit": "lint-staged",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "build-storybook": "build-storybook -c .storybook -o www",
    "deploy-storybook": "npm run build-storybook && storybook-to-ghpages --out=www"
  },
  "lint-staged": {
    "*.{js,jsx,json}": [
      "prettier --write",
      "git add"
    ],
    "*.{scss}": [
      "stylelint --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fengyinchao/RCK.git"
  },
  "keywords": [
    "React",
    "UI",
    "Component"
  ],
  "author": "fengyinchao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fengyinchao/RCK/issues"
  },
  "homepage": "https://github.com/fengyinchao/RCK#readme",
  "dependencies": {
    "@trendmicro/react-buttons": "^1.3.1",
    "light-toast": "^0.1.8",
    "rc-dialog": "^7.3.0",
    "rc-menu": "^7.4.21",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "react-tabs": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.3.4",
    "@babel/plugin-transform-runtime": "7.3.4",
    "@babel/preset-env": "7.3.4",
    "@babel/preset-react": "7.0.0",
    "@storybook/react": "^5.1.0-alpha.4",
    "@storybook/storybook-deployer": "^2.8.1",
    "autoprefixer": "9.4.9",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.5",
    "cross-env": "5.2.0",
    "css-loader": "2.1.0",
    "eslint": "5.2.0",
    "eslint-plugin-prettier": "2.6.1",
    "eslint-plugin-react": "7.12.4",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file-loader": "3.0.1",
    "gulp": "4.0.0",
    "gulp-autoprefixer": "6.0.0",
    "gulp-babel": "8.0.0",
    "gulp-sass": "4.0.2",
    "husky": "0.14.3",
    "lint-staged": "7.2.0",
    "node-sass": "^4.11.0",
    "postcss-loader": "3.0.0",
    "prettier": "1.16.3",
    "rimraf": "2.6.3",
    "sass-loader": "7.1.0",
    "storybook-readme": "^4.0.5",
    "style-loader": "0.23.1",
    "url-loader": "1.1.2",
    "web-webpack-plugin": "4.3.0",
    "webpack": "4.16.3",
    "webpack-bundle-analyzer": "3.0.3",
    "webpack-cli": "3.1.0",
    "webpack-dev-server": "3.1.5"
  }
}
